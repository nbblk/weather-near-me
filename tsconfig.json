{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "types": [
      "node"
    ],
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src",
    //    "vite.config.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "baseUrl": "./src/",
  "paths": {
    "@/*": [
      "src/*"
    ]
    // "@App/*": [
    //   "app/*"
    // ],
    // "@Features/*": [
    //   "features/*"
    // ],
    // "@Pages/*": [
    //   "pages/*"
    // ],
    // "@APIs": [
    //   "common/apis/*"
    // ],
    // "@Assets": [
    //   "common/assets/*"
    // ],
    // "@Components": [
    //   "common/components/*"
    // ],
    // "@Hooks": [
    //   "common/hooks/*"
    // ],
    // "@Utils": [
    //   "common/utils/*"
    // ],
  }
}